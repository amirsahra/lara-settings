{
  "name": "amirsahra/lara-settings",
  "description": "Settings that are related to the program and not related to the framework.",
  "keywords": [
    "amirsahra",
    "laravel setting",
    "application setting"
  ],
  "homepage": "https://github.com/amirsahra/lara-settings",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Amirsahra\\LaraSettings\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Amirsahra\\laraSettings\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "amirsahra",
      "email": "amirhosein.sahra@gmail.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": ">=5.7.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
